{
        "git.realtimeGutters": false,
        "git.addedSign.text": "┃",
        "git.changedSign.text": "┃",
        "git.changedSign.hlGroup": "DiffChangeNB",
        "git.addedSign.hlGroup": "DiffAddNB",
        "git.removedSign.hlGroup": "DiffDeleteNB",
        "git.topRemovedSign.hlGroup": "DiffDeleteNB",
        "git.changeRemovedSign.hlGroup": "DiffDeleteNB",
        "diagnostic.checkCurrentLine" : true,
        "diagnostic.enable": false,
        "python.jediEnabled": true,
        "python.linting.pylintEnabled": false,
        "python.linting.flake8Enabled": true,
        "python.linting.pylintArgs": [
                "--load-plugins pylint_django"
        ],
        "emmet.includeLanguages": {
                "htmldjango": "html"
        }
}
